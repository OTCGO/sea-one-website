{
  "name": "my-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot --build-optimizer --common-chunk --extract-css",
    "analyzer:build": "ng build --prod --aot --build-optimizer --common-chunk --extract-css --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyzer": "npx webpack-bundle-analyzer dist/stats.json",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run my-app:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0-beta.1",
    "@angular/common": "^7.1.0-beta.1",
    "@angular/compiler": "^7.1.0-beta.1",
    "@angular/core": "^7.1.0-beta.1",
    "@angular/forms": "^7.1.0-beta.1",
    "@angular/http": "^7.1.0-beta.1",
    "@angular/platform-browser": "^7.1.0-beta.1",
    "@angular/platform-browser-dynamic": "^7.1.0-beta.1",
    "@angular/platform-server": "^7.0.2",
    "@angular/router": "^7.1.0-beta.1",
    "@cityofzion/neon-js": "^3.11.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "core-js": "^2.4.1",
    "domino": "^2.1.0",
    "element-angular": "^0.7.4",
    "error-stack-parser": "^2.0.1",
    "express": "^4.15.2",
    "file-saver": "^1.3.8",
    "ramda": "^0.25.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "ts-loader": "^5.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.0.4",
    "@angular/compiler-cli": "^7.1.0-beta.1",
    "@angular/language-service": "^7.1.0-beta.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-loader": "^5.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
